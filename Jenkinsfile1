pipeline{
  agent any

  tools {
      maven 'M2_HOME'
        }
  stages {
     stage('checkout'){
       steps {
         echo 'checkout the code from GitRepo'
         git 'https://github.com/moodshanker/star-agile-insurance-project.git'
                    }
            }
   

     stage('Build the  Application'){
               steps {
                   echo "Cleaning.... Compiling......Testing.........Packaging"
                   sh 'mvn clean package'
                    }
                 }
                 
              }
          }    

